    <script>

        //! header animation

        document.addEventListener('contextmenu', event => event.preventDefault());
        // Get all the spans
        const spans = document.querySelectorAll('.ani h1');
        // Loop through the spans and add a class to show them one by one
        let i = 0;
        const showNextSpan = () => {
            // Remove the show class from all the spans
            spans.forEach(span => {
                span.classList.remove('show');
            });

            // Add the show class to the current span after a delay
            setTimeout(() => {
                spans[i].classList.add('show');
                i++;

                if (i >= spans.length) { // Check if i has reached the end of the array
                    i = 0; // Reset i to 0 if it has
                }

                setTimeout(showNextSpan, 1200); // Use a delay of 3000ms (2s fade-out + 1s delay)
            }, 1000); // Use a delay of 1000ms (1s delay)
        }


        // Call the function to start the animation
        showNextSpan();



