 <div class="card mb-4">
                <header class="card-header">
                    <div class="row gx-3">
                        <div class="col-lg-4 col-md-6 me-auto">
                            <input type="text" placeholder="Search..." class="form-control">
                        </div>
                        <div class="col-lg-2 col-md-3 col-6">
                            <select class="form-select">
                                <option>Status</option>
                                <option>Active</option>
                                <option>Disabled</option>
                                <option>Show all</option>
                            </select>
                        </div>
                        <div class="col-lg-2 col-md-3 col-6">
                            <select class="form-select">
                                <option>Show 20</option>
                                <option>Show 30</option>
                                <option>Show 40</option>
                            </select>
                        </div>
                    </div>
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>Coordinators</th>
                                    <th>Email</th>
                                    <th>Status</th>
                                    <th>Registered</th>
                                    <th class="text-end"> Action </th>
                                    <th class="text-end"> Action </th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each coordinator}}
                                <tr>
                                    <td width="40%">
                                        <a href="#" class="itemside">
                                            {{!-- <div class="left">
                                                <img src="assets/imgs/people/avatar1.jpg" class="img-sm img-avatar" alt="Userpic">
                                            </div> --}}
                                            <div class="info pl-3">
                                                <h6 class="mb-0 title">{{name}}</h6>
                                                <small class="text-muted"></small>
                                            </div>
                                        </a>
                                    </td>
                                    <td>{{email}}</td>
                                    {{#if isVerified}}
                                    <td><span class="badge rounded-pill alert-success">Active</span></td>
                                    {{else}}
                                    <td><span class="badge rounded-pill alert-danger">Blocked</span></td>

                                    {{/if}}
                                    <td>{{formatDate registeredAt}}</td>
                                    {{!-- <td class="text-end">
                                        <a href="#" class="btn btn-sm btn-brand rounded font-sm mt-15">View details</a>
                                    </td> --}}
                                    {{#if isVerified}}
                                    <td class="text-end">
                                        <a href="/admin/blockCord?id={{_id}}" class="btn btn-danger rounded font-sm mt-15" style="width: 100px;">Block</a>
                                    </td>
                                    {{else }}

                                     <td class="text-end">
                                        <a href="/admin/blockCord?id={{_id}}" class="btn btn-success rounded font-sm mt-15" style="width: 100px; color: #fff;">Un Block</a>
                                    </td>

                                    {{/if}}
                                </tr>
                                {{/each}}
                            </tbody>
                        </table> <!-- table-responsive.// -->
                    </div>
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
           